{
    "compilerOptions": {
        "target": "es5",
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "esModuleInterop": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "outDir": "./out",
        "experimentalDecorators": true,
    },
    "exclude": [
        "node_modules"
    ],
    "include": ["./**/*.ts"],
}